var icons = {
    header: "ui-icon-circle-arrow-e",
    headerSelected: "ui-icon-circle-arrow-s"
};
$(".contenu_presta #accordion").accordion({icons: icons, autoHeight: false});

// SAUVEGARDE
$(".contenu_presta #savePresta").click(function () {
    var form = $("form[name='presta']").serializeArray();

    /* Because serializeArray() ignores unset checkboxes and radio buttons: */
    form = form.concat($("form[name='presta'] input[type=checkbox]:not(:checked)").map(function () {
        return {"name": this.name, "value": 0}
    }).get());

    $.pnotify({title: 'Sauvegarde', text: '<span class="load" ><img src="{{ asset('bundles/lea/images/divers/load.gif') }}" /></span> Sauvegarde en cours', type: 'info', styling: 'jqueryui'});

    $.ajax({
        url: "./data",
        type: "POST",
        dataType: "json",
        data: form,
        success: function (data) {
            $.pnotify({title: 'Sauvegarde', text: data, type: 'success', styling: 'jqueryui'});

            $('.load').hide('fade');
        }
    });

    return false;

});

// EDITION
$(".contenu_presta #EditerPresta").click(function () {

    $.pnotify({title: 'Edition', text: 'Edition des livrables', type: 'info', styling: 'jqueryui'});

    $('.load').hide('fade');

    if ($('#imp_1').attr('checked') == 'checked') {
        window.open("./download/aic/" + {{presta.idPresta}} + "/1", '', "menubar=no, status=no, scrollbars=no, menubar=no, width=0, height=0");
    }
    if ($('#imp_2').attr('checked') == 'checked') {
        setTimeout(function () {
            window.open("./download/aic/" + {{presta.idPresta}} + "/2", '', "menubar=no, status=no, scrollbars=no, menubar=no, width=0, height=0")
        }, 1000);
    }
    return false;

});
</script>