// SAUVEGARDE		
$(".contenu_presta #savePresta").click(function() {
	var form  = $("form[name='presta']").serializeArray();

	/* Because serializeArray() ignores unset checkboxes and radio buttons: */
    form = form.concat(
		$("form[name='presta'] input[type=checkbox]:not(:checked)").map(
            function() {
                return {"name": this.name, "value": 0}
            }).get()
    );

   
	
	$.pnotify({
		title: 'Sauvegarde',
		text: '<span class="load" ><img src="{{ asset('bundles/lea/images/divers/load.gif') }}" /></span> Sauvegarde en cours',
		type: 'info',
		styling: 'jqueryui'
	});


	$.ajax({
		url : "./data",
		type : "POST",
		dataType : "json",
		data : form ,
		success: function(data){
			$.pnotify({
				title: 'Sauvegarde',
				text: data,
				type: 'success',
				styling: 'jqueryui'
			});

			$('.load').hide('fade', { direction: "up" }, 4);	
		}
	});

	return false;
});

