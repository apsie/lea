<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="EgwPrestataire" table="egw_prestataire">
    <id name="idPrestataire" type="integer" column="id_prestataire">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nom" type="string" column="nom" length="64" nullable="true"/>
    <field name="siret" type="string" column="siret" length="64" nullable="true"/>
    <field name="adresse" type="string" column="adresse" length="128" nullable="true"/>
    <field name="cp" type="string" column="cp" length="16" nullable="true"/>
    <field name="ville" type="string" column="ville" length="128" nullable="true"/>
    <field name="pays" type="string" column="pays" length="128" nullable="true"/>
    <field name="tel" type="string" column="tel" length="64" nullable="true"/>
    <field name="email" type="string" column="email" length="128" nullable="true"/>
  </entity>
</doctrine-mapping>
